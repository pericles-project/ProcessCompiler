  dem:description "Input slot of the atomic process Create RDF Triples corresponding to the edited metadata" ;
  dem:version "1" ;
  dem:dataType nar2:Metadata ;
  dem:isOptional "false" .

nar2:osCreateRDF_RDF a dem:OutputSlot ;
